<% unless @lent.empty? %>
	<% @lent.each do |l| %>
		$("#lend_requests_table > tbody:last").append("raw(<tr data-time='<%= l.updated_at.to_i %>'><td><%= BookDetail.find(UserBook.find(l.user_book_id).book_detail_id).book_name %></td><td><%= UserBook.find(l.user_book_id).rental_price %></td><td><%= l.status %></td></tr>)");
	<% end %>
<% end %>