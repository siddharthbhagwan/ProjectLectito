<h2> User List</h2>

<table class="table table-striped table-bordered table-condensed">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Dob</th>
    <th>Gender</th>
    <th>Phone No</th>
    <th>Current Status</th>
    <th>Last Update</th>
  </tr>

<%  @profile.each do |p| %>
	<tr>
		<td><%= p.user_first_name %></td>
		<td><%= p.user_last_name %></td>
		<td><%= p.DoB %></td>
		<td><%= p.gender %></td>
		<td><%= p.user_phone_no %></td>
		<td><%= p.current_status %></td>
		<td><%= p.last_update %></td>
	</tr>

<% end %>
</table>
<%= link_to "Back", admin_path, class:"btn btn-small" %>